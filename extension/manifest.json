{
  "manifest_version": 2,

  "name": "Mailgate Beta",
  "description": "Generates custom email forwarders for smart people.",
  "version": "1.0",

  "permissions": [
    "<all_urls>", "tabs", "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": {
      "19": "toolbar-button.png"
    },
    "default_popup": "popup.html"
  }
}